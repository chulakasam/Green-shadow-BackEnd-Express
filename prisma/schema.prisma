generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Field {
  fieldCode  String @id
  fieldName  String
  location   String
  fieldSize  Int
  fieldImg01 String
  fieldImg02 String
}

model Vehicle {
  LicenseNo   String @id
  VehicleCode String @unique
  Category    String
  Status      String
  FuelType    String
  Remark      String
}
